import tkinter as tk
def submit_form():
 print("Submitted successfully") 
 name = name_entry.get()
 age = age_entry.get()
 gender = gender_entry.get()
 email = email_entry.get()
 phone = phone_entry.get()
 dob = dob_entry.get()
 father_name = father_name_entry.get()
 father_phno = father_phno_entry.get()
 father_occupation = father_occupation_entry.get()
 address = address_entry.get("1.0", "end-1c")
 deg = deg_entry.get()
 spl = spl_entry.get()
 deyear = deyear_entry.get()
 cgpa = cgpa_entry.get()
 clgname = clgname_entry.get()
 dip = dip_entry.get()
 displ = displ_entry.get()
 diyear = diyear_entry.get()
 dicgpa = dicgpa_entry.get()
 diclgname = diclgname_entry.get()
 hsc_school_name = hsc_school_name_entry.get()
 hsc_mark = hsc_mark_entry.get()
 sslc_school_name = sslc_school_name_entry.get()
 sslc_mark = sslc_mark_entry.get()
 aadhaar = aadhaar_entry.get()
 pan = pan_entry.get()
 backlogs = backlogs_entry.get()
 internship = internship_entry.get()
 
 print("Name:", name)
 print("Age:", age)
 print("Gender:", gender)
 print("Email:", email)
 print("Phone Number:", phone)
 print("Date of Birth:", dob)
 print("Father's Name:", father_name)
 print("Father's Phone Number:",father_phno)
 print("Father's Occupation:", father_occupation)
 print("Address:", address)
 print("Degree:",deg)
 print("Specialization:",spl)
 print("Year of Passing:",deyear)
 print("CGPA:",cgpa)
 print("College Name:",clgname)
 print("Diplomo:",dip)
 print("Specialization:",displ)
 print("Year of Passing:",diyear)
 print("CGPA:",dicgpa)
 print("College Name:",diclgname)
 print("School Name:", hsc_school_name)
 print("12th Marks:", hsc_mark)
 print("10th School Name:", sslc_school_name)
 print("10th Marks:", sslc_mark)
 print("Aadhaar Number:", aadhaar)
 print("PAN Card:",pan)
 print("Backlogs:",backlogs)
 print("Internship:",internship)
 
root = tk.Tk()
root.title("Interview Applicationn Form")
# Heading
heading_label = tk.Label(root, text="Interview Application Form", font=("Helvetica", 16, 
"bold"))
heading_label.grid(row=0, columnspan=2, pady=10)
# Name
name_label = tk.Label(root, text="Name:")
name_label.grid(row=1, column=0)
name_entry = tk.Entry(root)
name_entry.grid(row=1, column=1)
# Age
age_label = tk.Label(root, text="Age:")
age_label.grid(row=2, column=0)
age_entry = tk.Entry(root)
age_entry.grid(row=2, column=1)
# Gender Entry
gender_label = tk.Label(root, text="Gender:")
gender_label.grid(row=3, column=0)
gender_entry = tk.Entry(root)
gender_entry.grid(row=3, column=1)
# Email
email_label = tk.Label(root, text="Email:")
email_label.grid(row=4, column=0)
email_entry = tk.Entry(root)
email_entry.grid(row=4, column=1)
# Phone Number
phone_label = tk.Label(root, text="Phone Number:")
phone_label.grid(row=5, column=0)
phone_entry = tk.Entry(root)
phone_entry.grid(row=5, column=1)
# Date of Birth
